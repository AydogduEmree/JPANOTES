package com.company.queries;

public class ProductQueries {
	
	public static final String findProductsAll = "SELECT p.productId, p.available, p.createdDate, p.name, p.unitPrice, p.updatedDate FROM Product p";

	public static final String findProducts = "SELECT P FROM PRODUCT P WHERE P.unitPrice > :unitPrice";

	public static final String findGroupByCategory = "SELECT P.category, p.categoryName, AVG(p.unitPrice) FROM PRODUCT P Group by p.category, p.categoryName";

}
